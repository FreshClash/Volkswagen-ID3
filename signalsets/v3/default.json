{ "commands": [
"hdr": "FC007B",
      // 'rax' (Empfangsadresse) wurde nicht angegeben, wird weggelassen.
      "cmd": {
        "22": "028C" // Service 22 (Read Data By Identifier), PID/DID 028C
      },
      // 'freq' wurde nicht angegeben, Annahme: 1 Sekunde (1 Hz). Bei Bedarf anpassen.
      "freq": 1,
      "signals": [
        {
          "id": "BMS_SOC",       // Eindeutige ID fÃ¼r das Signal
          "path": "Battery",     // Logischer Pfad zur Gruppierung
          "fmt": {
            "len": 8,          // Annahme: 'aa' ist das erste Datenbyte (8 Bits)
            "max": 100,        // Maximal erwarteter Wert (Prozent)
            "mul": 0.4,        // Multiplikator basierend auf Formel aa / 2.5 => aa * 0.4
            "unit": "percent"  // Einheit ist Prozent
          },
          "name": "SOC BMS" // Menschenlesbarer Name (State of Charge Battery Management System)
        }
      ]
]
}
